{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react-jsx",
      "lib": ["DOM","DOM.Iterable","ESNext"],
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node", "jest", "@testing-library/jest-dom"],
    },
    "include": ["src"],
  
  }